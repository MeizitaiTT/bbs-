<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.bbs.model.Subject" table="subject" >
        <id name="id" type="int">
        <column name="Id" />     
            <generator class="identity" />
        </id>
		<property name="subjectName" type="java.lang.String">
        	<column name="subjectName" />
        </property>
        <property name="subjectDescription" type="java.lang.String">
        	<column name="subjectDescription" />
        </property>
        <property name="buildDate" type="date">
        	<column name="buildDate" />
        </property>        
        <property name="managerId" type="int">
        	<column name="managerId" />
        </property>
        <property name="status" type="int">
        	<column name="status" />
        </property>
		<property name="imagepath" type="java.lang.String">
        	<column name="imagepath" />
        </property>
        
    </class>
</hibernate-mapping>
